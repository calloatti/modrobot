		<methods>PROCEDURE Resize
*!* resize

thisform.edit1.width = (thisform.width - 36) / 2
thisform.edit2.width = (thisform.width - 36) / 2

thisform.edit2.left = thisform.edit1.width + 24

thisform.label1.width = thisform.edit1.width
thisform.label2.width = thisform.edit2.width
thisform.label2.left  = thisform.edit2.left


thisform.edit1.height = thisform.height - 12 - thisform.edit1.top
thisform.edit2.height = thisform.height - 12 - thisform.edit2.top

if not empty(thisform.p1)

	thisform.label1.caption	= _shortenpathtocontrolwidth(thisform.p1, thisform.label1)

endif

if not empty(thisform.p2)

	thisform.label2.caption	= _shortenpathtocontrolwidth(thisform.p2, thisform.label2)

endif
ENDPROC
PROCEDURE init2

thisform.label1.caption	= ''
thisform.label2.caption	= ''

if not empty(thisform.p1)

	thisform.edit1.value	= filetostr(thisform.p1)

endif

if not empty(thisform.p2)

	thisform.edit2.value	= filetostr(thisform.p2)

endif

thisform.resize()
ENDPROC
